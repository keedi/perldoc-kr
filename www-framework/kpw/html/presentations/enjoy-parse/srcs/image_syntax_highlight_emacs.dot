(defvar extreme-perl-syntax-font-lock-keywords
  '((".+" (0 (prog1 nil
               (extreme-perl-syntax-remove-region
                (match-beginning 0) (match-end 0)))))
    ("\\(mummy\\)\\>"
     (0 (prog1 nil
          (mummy-duck-region (match-beginning 1) (match-end 1)))))
    ("\\(duck1\\)\\>"
     (0 (prog1 nil
          (chick1-duck-region (match-beginning 1) (match-end 1)))))
    ("\\(duck2\\)\\>"
     (0 (prog1 nil
          (chick2-duck-region (match-beginning 1) (match-end 1)))))
    ("\\(rune\\)\\>"
     (0 (prog1 nil
          (rune-duck-region (match-beginning 1) (match-end 1)))))
    ("\\($whitecatz\\)\\>"
     (0 (prog1 nil
          (whitecatz-region (match-beginning 1) (match-end 1)))))
    ("\\($JEEN\\)\\>"
     (0 (prog1 nil
          (jeen-region (match-beginning 1) (match-end 1)))))))

(defun jeen-region (beg end)
  (put-text-property beg end 'display jeen-image)
)


(defun whitecatz-region (beg end)
  (put-text-property beg end 'display whitecatz-image)
)


(defun mummy-duck-region (beg end)
  (put-text-property beg end 'display mummy-duck-image)
)


(defun chick1-duck-region (beg end)
  (put-text-property beg end 'display chick1-duck-image)
  )

(defun chick2-duck-region (beg end)
  (put-text-property beg end 'display chick2-duck-image)
  )


(defun rune-duck-region (beg end)
  (put-text-property beg end 'display rune-duck-image)
  )

  
(defun extreme-perl-syntax-remove-region (beg end)
  (let (pos)
    (while (setq pos (text-property-any beg end 'display jeen-image))
      (remove-text-properties
       pos
       (or (next-single-property-change pos 'display) end)
       '(display)))
    )
  (let (pos)
    (while (setq pos (text-property-any beg end 'display mummy-duck-image))
      (remove-text-properties
       pos
       (or (next-single-property-change pos 'display) end)
       '(display)))
    )
  (let (pos)
    (while (setq pos (text-property-any beg end 'display chick1-duck-image))
      (remove-text-properties
       pos
       (or (next-single-property-change pos 'display) end)
       '(display)))
    )
  (let (pos)
    (while (setq pos (text-property-any beg end 'display chick2-duck-image))
      (remove-text-properties
       pos
       (or (next-single-property-change pos 'display) end)
       '(display)))
    )
  (let (pos)
    (while (setq pos (text-property-any beg end 'display rune-duck-image))
      (remove-text-properties
       pos
       (or (next-single-property-change pos 'display) end)
       '(display)))
    )

)
  
(define-minor-mode extreme-perl-syntax-mode
  "Display thick image syntax hightlight"
  :lighter " Perl"
  (if extreme-perl-syntax-mode
      (progn
        (save-restriction
          (widen)
          (let ((font-lock-keywords extreme-perl-syntax-font-lock-keywords))
            (font-lock-fontify-buffer)))
        (font-lock-add-keywords nil extreme-perl-syntax-font-lock-keywords))
    (font-lock-remove-keywords nil extreme-perl-syntax-font-lock-keywords)
    (save-restriction
      (widen)
      (let ((modified-p (buffer-modified-p)))
        (jeen-region (point-min) (point-max))
        (whitecatz-region (point-min) (point-max))
        (mummy-duck-region (point-min) (point-max))
        (chick1-duck-region (point-min) (point-max))        
        (chick2-duck-region (point-min) (point-max))        
        (rune-duck-region (point-min) (point-max))
        (set-buffer-modified-p modified-p)))))

(defun extreme-perl-syntax-mode-turn-on ()
  "Turn on `extreme-perl-syntax-mode'."
  (interactive)
  (extreme-perl-syntax-mode 1))
  
